odoo.define('@industry_fsm_stock/../tests/tours/industry_fsm_stock_test_tour', ['@web/core/registry', '@web_tour/tour_service/tour_utils'], function (require) {
'use strict';
let __exports = {};
/** @odoo-module **/
const { registry } = require("@web/core/registry");
